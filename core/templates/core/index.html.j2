{% extends "base.html.j2" %}
{% load static %}

<link rel="stylesheet" href="{% static 'core/css/styles.css' %}">
<img src="{% static 'core/img/placeholder.jpg' %}" alt="placeholder"/>

{% if camera_list %}
    <ul>
        {% for camera in camera_list %}
            <li>{{ camera.name }}</li>
        {% endfor %}
    </ul>
{% else %}
    <p>No cameras are available.</p>
{% endif %}